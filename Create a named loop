//Task 3: Create a named loop

public class NamedLoop {
    public static void main(String[] args) {
        loop1:
        for (int i = 0; i < 6; i++) {
            for (int j = 0; j < 6; j++) {
                if (i == 3)
                    break loop1;
                System.out.println("i = " + i + " j = " + j);
            }
        }
    }
    
}
